{
    "name": "paq",
    "buildsystem": "simple",
    "only-arches": ["x86_64"],
    "build-commands": [
          "yasm paq7asm-x86_64.asm -f elf -m amd64",
          "g++ $CFLAGS -DUNIX paq8o9.cpp paq7asm-x86_64.o -o paq8o",
          "install -Dm755 paq8o -t /app/plugin/paq"
        ],
    "cleanup": ["/plugin"],
    "sources": [
        {
            "type": "archive",
            "url": "https://cs.fit.edu/~mmahoney/compression/paq8o9.zip",
            "sha256": "53eafa4797d72b6d5630cfa9b43e69b3de58f01119bdf8f23b6d3d940a9d2336"
        }
    ],
    "modules": [
        {
            "name": "yasm",
            "build-commands": ["make check"],
            "cleanup": ["*"],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://www.tortall.net/projects/yasm/releases/yasm-1.3.0.tar.gz",
                    "sha256": "3dce6601b495f5b3d45b59f7d2492a340ee7e84b5beca17e48f862502bd5603f"
                }
            ]

        }
    ]
}
